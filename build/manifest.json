{
  "manifest_version": 3,
  "name": "Social Bias Analyzer Beta",
  "version": "1.0",
  "description": "Analyzes web pages for social bias using AI models",
  "permissions": ["activeTab", "storage", "alarms", "scripting", "tabs"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
